architecture ARH of numarator_an is
signal count_an: unsigned(6 downto 0) :="0010111";
signal b: std_logic :='0';
begin

process(clk)
begin
	if enable='1' then
		if rising_edge(clk) then
			if count_an=99 then
				count_an&lt;="0010111";
			elsif luna=12 and zi=31 then
				count_an&lt;=count_an+1;
			end if;
			
		end if;

		if count_an=24 or count_an=28 or count_an=32 or count_an=36 or count_an=40 or count_an=44 or count_an=48 or count_an=52 or count_an=56 or count_an=60 or count_an=64 or count_an=68 or count_an=72 or count_an=76 or count_an=80 or count_an=84 or count_an=88 or count_an=92 or count_an=96 then
			b&lt;='1';
		else
			b&lt;='0';
		end if;
	end if;
end process;



m&lt;="0010";
s&lt;="0000";
an&lt;=std_logic_vector(count_an);
bisect&lt;=b;


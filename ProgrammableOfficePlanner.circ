		a1&lt;='1';b1&lt;='1';c1&lt;='1';d1&lt;='1';e1&lt;='1';f1&lt;='1';g1&lt;='0';

	when "101" =&gt; --Vineri
		a0&lt;='0';b0&lt;='1';c0&lt;='1';d0&lt;='1';e0&lt;='1';f0&lt;='1';g0&lt;='1';
		a1&lt;='0';b1&lt;='1';c1&lt;='1';d1&lt;='0';e1&lt;='0';f1&lt;='0';g1&lt;='0';

	when "110" =&gt; --Sambata
		a0&lt;='1';b0&lt;='0';c0&lt;='1';d0&lt;='1';e0&lt;='0';f0&lt;='1';g0&lt;='1';
		a1&lt;='1';b1&lt;='1';c1&lt;='1';d1&lt;='0';e1&lt;='1';f1&lt;='1';g1&lt;='1';

	when others =&gt; --Duminica
		a0&lt;='1';b0&lt;='1';c0&lt;='1';d0&lt;='1';e0&lt;='1';f0&lt;='1';g0&lt;='0';
		a1&lt;='0';b1&lt;='1';c1&lt;='1';d1&lt;='1';e1&lt;='1';f1&lt;='1';g1&lt;='0';

	end case;

end process;


  <vhdl name="temp_generator">--------------------------------------------------------------------------------&#13;
-- Project :&#13;
-- File    :&#13;
-- Autor   :&#13;
-- Date    :&#13;
--&#13;
--------------------------------------------------------------------------------&#13;
-- Description :&#13;
--&#13;
--------------------------------------------------------------------------------&#13;
library ieee;
use ieee.std_logic_1164.all;
use ieee.numeric_std.all;

entity temp_generator is
  Port (
    enable: in std_logic;
    luna: in std_logic_vector(3 downto 0);
    ora: in std_logic_vector(4 downto 0);
    temperature: out std_logic_vector(4 downto 0)
  );
end temp_generator;

architecture behavioral of temp_generator is

  function random_number(min: natural; max: natural) return std_logic_vector is
  variable range1: natural := max - min + 1;
  variable random_num: natural;
begin

  random_num := (min + natural(random_num) mod range1);
  return std_logic_vector(to_unsigned(random_num, 5));
end function;



begin
  generate_temperature:
    process(enable, luna, ora)
    variable random_num: std_logic_vector(4 downto 0);
  begin
    if enable = '1' then
      if luna = "0001" or luna = "0010" or luna = "1100" then -- iarna
        if ora &gt;= "01000" and ora &lt;= "01111" then -- intervalul 8-12
          random_num := random_number(2, 7);
        elsif ora &gt;= "1101" and ora &lt;= "10100" then -- intervalul 13-20
          random_num := random_number(6, 9);
        else
          random_num := random_number(0, 5);
        end if;
      elsif luna = "0011" or luna = "0100" or luna = "0101" then -- primavara
        if ora &gt;= "01000" and ora &lt;= "01111" then -- intervalul 8-12
          random_num := random_number(8, 14);
        elsif ora &gt;= "1101" and ora &lt;= "10100" then -- intervalul 13-20
          random_num := random_number(13, 20);
        else
          random_num := random_number(6, 10);
        end if;
      elsif luna = "0110" or luna = "0111" or luna = "1000" then -- vara
        if ora &gt;= "01000" and ora &lt;= "01111" then -- intervalul 8-12
          random_num := random_number(15, 20);
        elsif ora &gt;= "1101" and ora &lt;= "10100" then -- intervalul 13-20
          random_num := random_number(21, 30);
        else
          random_num := random_number(10, 17);
        end if;
      elsif luna = "1001" or luna = "1010" or luna = "1011" then -- toamna
        if ora &gt;= "01000" and ora &lt;= "01111" then -- intervalul 8-12
          random_num := random_number(6, 12);
        elsif ora &gt;= "1101" and ora &lt;= "10100" then -- intervalul 13-20
          random_num := random_number(10, 18);
        else
          random_num := random_number(4, 8);
        end if;
      else
        random_num := random_number(0, 31);
      end if;
      
      temperature &lt;= random_num;
    end if;
  end process generate_temperature;
end behavioral;

</vhdl>

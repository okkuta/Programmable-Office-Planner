ENTITY numarator_an IS&#13;
port(
	clk: in std_logic;
	enable: in std_logic;

	luna: in std_logic_vector(3 downto 0);
	zi: in std_logic_vector(4 downto 0);

	m: out std_logic_vector(3 downto 0);
	s: out std_logic_vector(3 downto 0);
	an: out std_logic_vector(6 downto 0);
	bisect: out std_logic
	);


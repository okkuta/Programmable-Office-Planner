  <vhdl name="unitate_de_control">--------------------------------------------------------------------------------&#13;
-- Project :&#13;
-- File    :&#13;
-- Autor   :&#13;
-- Date    :&#13;
--&#13;
--------------------------------------------------------------------------------&#13;
-- Description :&#13;
--&#13;
--------------------------------------------------------------------------------&#13;
&#13;
LIBRARY ieee;
USE ieee.std_logic_1164.all;
use ieee.std_logic_unsigned.all;

entity unitate_de_control is

port(
	clk: in std_logic;
	start: in std_logic;
	setare: in std_logic;
	afisare: in std_logic;
	confirm: in std_logic;
	reset: in std_logic;
	verif: in std_logic;

	OFF: out std_logic;
	ONN: out std_logic;
	DISPLAY: out std_logic;
	ALARM: out std_logic;
	ORA: out std_logic;
	MIN: out std_logic;
	DIFUZOR: out std_logic
	);
	
end unitate_de_control;


architecture ARH of unitate_de_control is
type STATE is (S_IDLE,S_ON,S_ALARMA,S_AFIS,S_ORA,S_MIN);
signal stare,stare_urm: STATE;

begin

process(clk,reset)
begin
	if reset='1' then
		stare&lt;=S_IDLE;
	elsif rising_edge(clk) then
		stare&lt;=stare_urm;
	end if;

end process;

process(stare,start,setare,afisare,confirm,reset,verif)
begin
	case stare is
		when S_IDLE =&gt;
			if start='1' then
				stare_urm&lt;=S_ON;
			else
				stare_urm&lt;=S_IDLE;
			end if;
		when S_ON =&gt;
			if setare='1' then
				stare_urm&lt;=S_ALARMA;
			elsif afisare='1' then 
				stare_urm&lt;=S_AFIS;
			else
				stare_urm&lt;=S_ON;
			end if;

			
		when S_ALARMA =&gt;
			if confirm='1' then
				stare_urm&lt;=S_ORA;
			else
				stare_urm&lt;=S_ALARMA;
			end if;

			
		when S_ORA =&gt;
			if confirm='1' then
				stare_urm&lt;=S_MIN;
			else
				stare_urm&lt;=S_ORA;
			end if;

			
		when S_MIN =&gt;
			if confirm='1' then
				stare_urm&lt;=S_ON;
			elsif reset='1' then
				stare_urm&lt;=S_IDLE;
			else
				stare_urm&lt;=S_MIN;
			end if;

			
		when S_AFIS =&gt;
			if setare='1' then
				stare_urm&lt;=S_ALARMA;
			else
				stare_urm&lt;=S_AFIS;
			end if;


	end case;
end process;

process(stare,start,setare,afisare,confirm,reset,verif)
begin

	OFF&lt;='0'; ONN&lt;='0'; DISPLAY&lt;='0'; ALARM&lt;='0'; ORA&lt;='0'; MIN&lt;='0'; DIFUZOR&lt;='0';

	if verif='1' then
		DIFUZOR&lt;='1';
	end if;

	case stare is
		when S_IDLE =&gt; OFF&lt;='1';
		when S_ON =&gt; ONN&lt;='1';
		when S_ALARMA =&gt; ALARM&lt;='1';ONN&lt;='1';
		WHEN S_AFIS =&gt; DISPLAY&lt;='1';ONN&lt;='1';
		WHEN S_ORA =&gt; ORA&lt;='1';ONN&lt;='1';
		WHEN S_MIN =&gt; MIN&lt;='1';ONN&lt;='1';
			
	end case;

end process;



end architecture;
</vhdl>
